<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <style>
        body {
            margin: 0;
            padding: 0;
            font-family: Arial, sans-serif;
        }

        .map-container {
            width: 100%;
            height: 600px; 
            border: 2px solid #ccc; 
            border-radius: 8px; 
            overflow: hidden; 
        }

            .map-container iframe {
                width: 100%;
                height: 100%;
                border: none;
            }
    </style>
</head>
<body>
    <header>
        <h1>@ViewBag.Title</h1>
    </header>
    <main aria-labelledby="title">
        <div class="map-container">
            @*<iframe src="path/to/your/map/file.html" title="Map"></iframe>*@
        </div>
        <p>For map page</p>
    </main>
</body>
</html>
